{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app",
				"enablePullDownRefresh": true

			}
		}, {
			"path": "pages/communtiy/communtiy",
			"style": {
				"navigationBarTitleText": "社区",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/data/data",
			"style": {
				"navigationBarTitleText": "数据统计",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/me/me",
			"style": {
				"navigationBarTitleText": "我的",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/index/add",
			"style": {
				"navigationBarTitleText": "添加",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/index/detail",
			"style": {
				"navigationBarTitleText": "详情",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/index/search/search",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false,
				"app-plus": {
					"titleNView": {
						"searchInput": {
							"align": "left",
							"backgroundColor": "#fff",
							"borderRadius": "6px", //输入框圆角
							"placeholder": "请输入搜索内容",
							"disabled": false //disable时点击输入框不置焦，可以跳到新页面搜索
						}
					}
				}
			}
		}
	    
        ,{
            "path" : "pages/index/home",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "数据",
                "enablePullDownRefresh": false,
				"app-plus": {
					"bounce": "none", //关闭窗口回弹效果
					"titleNView": {
						"buttons": [ //原生标题栏按钮配置,
							{
								"fontSize": "18px",
								"text": "添加" //原生标题栏增加分享按钮，点击事件可通过页面的 onNavigationBarButtonTap 函数进行监听
							}
						]
					}
				}
            }
            
        }
        
        ,{
            "path" : "pages/me/login/login",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "登录",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/me/login/register",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "账号登录",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/me/login/forgetPassword",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "忘记密码",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},

	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#007AFF",
		"borderStyle": "black",
		"backgroundColor": "#FFFFFF",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/tabbar/home.png",
			"text": "首页"
		}, {
			"pagePath": "pages/data/data",
			"iconPath": "static/tabbar/data.png",
			"text": "数据统计"
		}, {
			"pagePath": "pages/communtiy/communtiy",
			"iconPath": "static/tabbar/community.png",
			"text": "社区"
		}, {
			"pagePath": "pages/me/me",
			"iconPath": "static/tabbar/me.png",
			"text": "我的"
		}]
	}

}